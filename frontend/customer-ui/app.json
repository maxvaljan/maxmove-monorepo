{
  "expo": {
    "name": "MaxMove Customer",
    "slug": "maxmove-customer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "maxmoveapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "react-native-webview"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "install": {
      "exclude": ["react-native-webview", "@stripe/stripe-react-native"]
    }
  }
}
